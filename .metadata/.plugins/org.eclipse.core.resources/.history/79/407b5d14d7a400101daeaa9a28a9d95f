public interface VerificationDocumentService {
    VerificationDocument uploadDocument(VerificationDocument document);
    VerificationDocument getDocumentById(Long id);
    List<VerificationDocument> getAllDocuments();
    VerificationDocument updateStatus(Long id, String status);
    void deleteDocument(Long id);
}
